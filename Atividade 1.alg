algoritmo "Atividade 1"

var
 n, antecessor, sucessor: inteiro

inicio

       escreval ("Por favpr informe um número: ")
       leia(n)
       
       antecessor <- n-1
       sucessor <- n+1
       
       escreval("Número informado: ", n)
       escreval("Antecessor: ", Antecessor)
       escreval("Sucessor: ", Sucessor)
       
fimalgoritmo
