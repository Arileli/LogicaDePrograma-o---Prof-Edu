algoritmo "semnome"

var
   numero: real
inicio

      escreval("Digite um número? ")
      leia(numero)
      
      se ((numero >=20)e (numero <= 90)) entao
         escreval("O número está entre 20 e 90")
      senao
           escreval("O número não está entre 20 e 90")
      fimse
fimalgoritmo
